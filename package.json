    "types": "rm -rf types && tsc -d --outDir types --noEmit false --emitDeclarationOnly true",
    "demo": "rollup -c rollup.demo.config.js",
    "production": "npm run build && npm run types",
    "lint": "eslint src"
  },
  "keywords": [
