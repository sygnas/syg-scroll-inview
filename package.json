{
  "name": "@sygnas/scroll-inview",
  "version": "3.1.0",
  "description": "Scroll in viewport add data attribute.",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "types/index.d.ts",
  "browser": "dist/scroll-inview.js",
  "moduleName": "sygScrollInview",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/sygnas/syg-scroll-inview.git"
  },
  "scripts": {
    "start": "npm run watch",
    "watch": "rollup -w -c rollup.config.js",
